@model IEnumerable<CARS.Models.Incidencia>

@{
    ViewBag.Title = "Home Page";
}


    <ul class="nav nav-tabs">
        <li class="nav-item">
            <a class="nav-link active" data-toggle="tab" href="#pendientes">Pendientes</a>
        </li>
        <li class="nav-item">
            <a class="nav-link" data-toggle="tab" href="#finalizadas">Finalizadas</a>
        </li>

    </ul>
    <div id="myTabContent" class="tab-content">
        <div class="tab-pane fade show active" id="pendientes">

            <div class="row">


                <table class="table">
                    <tr>
                        <th>
                            @Html.DisplayNameFor(model => model.Estado)
                        </th>
                        <th>
                            @Html.DisplayNameFor(model => model.FechaInicio)
                        </th>
                        <th>
                            @Html.DisplayNameFor(model => model.FechaFin)
                        </th>
                        <th>
                            @Html.DisplayNameFor(model => model.FechaSugerida)
                        </th>
                        <th>
                            @Html.DisplayNameFor(model => model.Descripcion)
                        </th>
                        <th>
                            @Html.DisplayNameFor(model => model.DireccionSugerida)
                        </th>
                        <th>
                            @Html.DisplayNameFor(model => model.Kilometraje)
                        </th>
                        <th></th>
                    </tr>

                    @foreach (var item in Model)
                    {
                        <tr>
                            <td>
                                @Html.DisplayFor(modelItem => item.Estado)
                            </td>
                            <td>
                                @Html.DisplayFor(modelItem => item.FechaInicio)
                            </td>
                            <td>
                                @Html.DisplayFor(modelItem => item.FechaFin)
                            </td>
                            <td>
                                @Html.DisplayFor(modelItem => item.FechaSugerida)
                            </td>
                            <td>
                                @Html.DisplayFor(modelItem => item.Descripcion)
                            </td>
                            <td>
                                @Html.DisplayFor(modelItem => item.DireccionSugerida)
                            </td>
                            <td>
                                @Html.DisplayFor(modelItem => item.Kilometraje)
                            </td>
                            <td>
                                @Html.ActionLink("Edit", "Edit", new { id = item.Id }) |
                                @Html.ActionLink("Details", "Details", new { id = item.Id }) |
                                @Html.ActionLink("Delete", "Delete", new { id = item.Id })
                            </td>
                        </tr>
                    }

                </table>
            </div>
        </div>
